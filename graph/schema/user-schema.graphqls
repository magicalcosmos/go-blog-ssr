# GraphQL schema example
#
# https://gqlgen.com/getting-started/
scalar Time
type User {
  id: ID!
  username: String!
  email: String!
  status: Int!
  created: Time!
}

type Query {
  users: [User!]!
}

input NewUser {
  username: String!
  password: String!
}
type Mutation {
  login(input: NewUser!): Boolean!
  createUser(input: NewUser!): User!
}
